dna_string = ''
with open('rosalind_kmp.txt') as fasta:
    next(fasta)  # ignora a primeira linha
    for line in fasta:
        dna_string += line
print dna_string

fail_array = [0] * len(dna_string)
j = 1
for i in range(2, len(dna_string)):
    while j + i <= len(dna_string):
        sub_string = dna_string[j:j + i]
        if sub_string == dna_string[0:(i - j + 1)]:
            fail_array[i - 1] += 1
        j += 1
